    param:hasParameter :analyze-step-duration .

:evaluate-step a pwo:Step ;
    taskex:isExecutedIn :evaluation-activity ;
    pwo:hasNextStep :treat-step ;
    param:hasParameter :evaluate-step-duration .

:treat-step a pwo:Step ;
    taskex:isExecutedIn :treatment-activity ;
    param:hasParameter :treat-step-duration .

:context-description-activity a hero:AssessmentActivity ;
