:risk-assessment-museum a pwo:WorkflowExecution ;
    pwo:involvesActivity 
        :context-description-activity ,
        :identification-activity ,
        :analysis-activity ,
        :evaluation-activity ,
        :treatment-activity ;
    pwo:executes :workflow-museum .

:workflow-museum a pwo:Workflow ;
    pwo:hasFirstStep :context-step ;
    pwo:hasStep 
        :identify-step ,
        :analyze-step ,
        :evaluate-step ,
        :treat-step .

:context-step a pwo:Step ;
