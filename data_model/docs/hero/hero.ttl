@prefix : <http://purl.org/sirius/ontology/hero#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://purl.org/sirius/ontology/hero> .

<http://purl.org/sirius/ontology/hero> rdf:type owl:Ontology ;
                                        owl:imports <http://www.ontologydesignpatterns.org/cp/owl/classification.owl> ,
                                                    <http://www.ontologydesignpatterns.org/cp/owl/observation.owl> ,
                                                    <http://www.ontologydesignpatterns.org/cp/owl/participation.owl> ,
                                                    <http://www.ontologydesignpatterns.org/cp/owl/region.owl> ,
                                                    <http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl> ,
                                                    <http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl> ,
                                                    <http://xmlns.com/foaf/0.1/> ;
                                        <http://purl.org/dc/elements/1.1/creator> "Barzaghi, Sebastian [0000-0002-0799-1527]" ;
                                        <http://purl.org/dc/elements/1.1/date> "2023-11-30"^^xsd:date ;
                                        <http://purl.org/dc/elements/1.1/description> "The Heritage Risk Assessment Ontology (HeRO, https://purl.org/sirius/ontology/hero) is an OWL 2 DL ontology that aims to provide a framework for the formal description of risk assessment activities related to heritage."@en ;
                                        <http://purl.org/dc/elements/1.1/title> "Heritage Risk Assessment Ontology"@en ;
                                        <http://purl.org/dc/terms/rights> "This work is distributed under a Creative Commons Attribution License (http://creativecommons.org/licenses/by/4.0/)."@en ;
                                        owl:versionInfo "1.0.0" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
<http://purl.org/dc/elements/1.1/date> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/rights
<http://purl.org/dc/terms/rights> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/sirius/ontology/hero/analyses
<http://purl.org/sirius/ontology/hero/analyses> rdf:type owl:ObjectProperty ;
                                                rdfs:subPropertyOf <http://purl.org/sirius/ontology/hero/assesses> ;
                                                rdfs:comment "A relationship between an analysis and a risk that is being analysed."@en ;
                                                rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                rdfs:label "analyses"@en .


###  http://purl.org/sirius/ontology/hero/assesses
<http://purl.org/sirius/ontology/hero/assesses> rdf:type owl:ObjectProperty ;
                                                rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#forEntity> ;
                                                rdfs:comment "A relationship between an assessment activity and something assessed by it, such as a criterion."@en ;
                                                rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                rdfs:label "assesses"@en .


###  http://purl.org/sirius/ontology/hero/assigns
<http://purl.org/sirius/ontology/hero/assigns> rdf:type owl:ObjectProperty ;
                                               rdfs:subPropertyOf <http://purl.org/sirius/ontology/hero/assesses> ;
                                               rdfs:comment "A relationship between a value assessment and an asset value that is being assessed."@en ;
                                               rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                               rdfs:label "assigns"@en .


###  http://purl.org/sirius/ontology/hero/describes
<http://purl.org/sirius/ontology/hero/describes> rdf:type owl:ObjectProperty ;
                                                 rdfs:subPropertyOf <http://purl.org/sirius/ontology/hero/hasParticipant> ,
                                                                    <http://www.ontologydesignpatterns.org/cp/owl/situation.owl#isSettingFor> ;
                                                 rdfs:comment "A relationship between an assessment activity and the thing described in that assessment, such as an asset."@en ;
                                                 rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                 rdfs:label "describes"@en .


###  http://purl.org/sirius/ontology/hero/documents
<http://purl.org/sirius/ontology/hero/documents> rdf:type owl:ObjectProperty ;
                                                 owl:inverseOf <http://purl.org/sirius/ontology/hero/isDocumentedBy> ;
                                                 rdfs:comment "A relationship between a document and something it documents."@en ;
                                                 rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                 rdfs:label "documents"@en .


###  http://purl.org/sirius/ontology/hero/hasObservation
<http://purl.org/sirius/ontology/hero/hasObservation> rdf:type owl:ObjectProperty ;
                                                      rdfs:subPropertyOf <http://purl.org/sirius/ontology/hero/assesses> ,
                                                                         <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#hasObservation> ;
                                                      owl:inverseOf <http://purl.org/sirius/ontology/hero/isObservationOf> ;
                                                      rdfs:comment "A relationship between an assessment activity and an observation occurring during said activity."@en ;
                                                      rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                      rdfs:label "has observation"@en .


###  http://purl.org/sirius/ontology/hero/hasParameter
<http://purl.org/sirius/ontology/hero/hasParameter> rdf:type owl:ObjectProperty ;
                                                    rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#hasParameter> ;
                                                    rdfs:comment "A relationship between an observation happening during a contextualisation activity and a contextual parameter that is being observed as part of the context."@en ;
                                                    rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                    rdfs:label "has parameter"@en .


###  http://purl.org/sirius/ontology/hero/hasParticipant
<http://purl.org/sirius/ontology/hero/hasParticipant> rdf:type owl:ObjectProperty ;
                                                      rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/participation.owl#hasParticipant> ;
                                                      owl:inverseOf <http://purl.org/sirius/ontology/hero/isParticipantIn> ;
                                                      rdfs:comment "A relationship between an assessment activity and a participant that takes part in it."@en ;
                                                      rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                      rdfs:label "has participant"@en .


###  http://purl.org/sirius/ontology/hero/identifies
<http://purl.org/sirius/ontology/hero/identifies> rdf:type owl:ObjectProperty ;
                                                  rdfs:subPropertyOf <http://purl.org/sirius/ontology/hero/assesses> ;
                                                  rdfs:comment "A relationship between a identification activity and a risk that is being identified."@en ;
                                                  rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                  rdfs:label "identifies"@en .


###  http://purl.org/sirius/ontology/hero/isClassifiedBy
<http://purl.org/sirius/ontology/hero/isClassifiedBy> rdf:type owl:ObjectProperty ;
                                                      rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/classification.owl#isClassifiedBy> ;
                                                      rdfs:comment "A relationship between a risk and another concept that may classify it, such as an agent of deterioration, a layer, or a type."@en ;
                                                      rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                      rdfs:label "is classified by"@en .


###  http://purl.org/sirius/ontology/hero/isClassifiedByAgent
<http://purl.org/sirius/ontology/hero/isClassifiedByAgent> rdf:type owl:ObjectProperty ;
                                                           rdfs:subPropertyOf <http://purl.org/sirius/ontology/hero/isClassifiedBy> ;
                                                           rdfs:comment "A relationship between a risk and an agent of deterioration."@en ;
                                                           rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                           rdfs:label "is classified by agent"@en .


###  http://purl.org/sirius/ontology/hero/isClassifiedByLayer
<http://purl.org/sirius/ontology/hero/isClassifiedByLayer> rdf:type owl:ObjectProperty ;
                                                           rdfs:subPropertyOf <http://purl.org/sirius/ontology/hero/isClassifiedBy> ;
                                                           rdfs:comment "A relationship between a risk and a layer."@en ;
                                                           rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                           rdfs:label "is classified by layer"@en .


###  http://purl.org/sirius/ontology/hero/isClassifiedByType
<http://purl.org/sirius/ontology/hero/isClassifiedByType> rdf:type owl:ObjectProperty ;
                                                          rdfs:subPropertyOf <http://purl.org/sirius/ontology/hero/isClassifiedBy> ;
                                                          rdfs:comment "A relationship between a risk and a type."@en ;
                                                          rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                          rdfs:label "is classified by type"@en .


###  http://purl.org/sirius/ontology/hero/isDocumentedBy
<http://purl.org/sirius/ontology/hero/isDocumentedBy> rdf:type owl:ObjectProperty ;
                                                      rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                      rdfs:label "is documented by" .


###  http://purl.org/sirius/ontology/hero/isObservationOf
<http://purl.org/sirius/ontology/hero/isObservationOf> rdf:type owl:ObjectProperty ;
                                                       rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#isObservationOf> ;
                                                       rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                       rdfs:label "is observation of"@en .


###  http://purl.org/sirius/ontology/hero/isParticipantIn
<http://purl.org/sirius/ontology/hero/isParticipantIn> rdf:type owl:ObjectProperty ;
                                                       rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/participation.owl#isParticipantIn> ;
                                                       rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                       rdfs:label "is participant in"@en .


###  http://purl.org/sirius/ontology/hero/isQuantifiedBy
<http://purl.org/sirius/ontology/hero/isQuantifiedBy> rdf:type owl:ObjectProperty ;
                                                      rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/region.owl#isRegionFor> ;
                                                      owl:inverseOf <http://purl.org/sirius/ontology/hero/quantifies> ;
                                                      rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                      rdfs:label "is quantified by"@en .


###  http://purl.org/sirius/ontology/hero/quantifies
<http://purl.org/sirius/ontology/hero/quantifies> rdf:type owl:ObjectProperty ;
                                                  rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/region.owl#hasRegion> ;
                                                  rdfs:comment "A relationship between an analysis activity and a measure considered during the analysis."@en ;
                                                  rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                  rdfs:label "quantifies"@en .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/sirius/ontology/hero/assessesPercentage
<http://purl.org/sirius/ontology/hero/assessesPercentage> rdf:type owl:DatatypeProperty ;
                                                          rdfs:comment "A property that assigns a value percentage to an asset which has been assigned a value in a value description."@en ;
                                                          rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                          rdfs:label "assessesPercentage"@en .


###  http://purl.org/sirius/ontology/hero/hasEstimate
<http://purl.org/sirius/ontology/hero/hasEstimate> rdf:type owl:DatatypeProperty ;
                                                   rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/region.owl#hasRegionDataValue> ;
                                                   rdfs:comment "A property that assigns a estimate value to a measure."@en ;
                                                   rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                   rdfs:label "has estimate"@en .


###  http://purl.org/sirius/ontology/hero/hasHighEstimate
<http://purl.org/sirius/ontology/hero/hasHighEstimate> rdf:type owl:DatatypeProperty ;
                                                       rdfs:subPropertyOf <http://purl.org/sirius/ontology/hero/hasEstimate> ;
                                                       rdfs:comment "A property that assigns a estimate value for the worst case scenario to a measure."@en ;
                                                       rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                       rdfs:label "has high estimate" .


###  http://purl.org/sirius/ontology/hero/hasLowEstimate
<http://purl.org/sirius/ontology/hero/hasLowEstimate> rdf:type owl:DatatypeProperty ;
                                                      rdfs:subPropertyOf <http://purl.org/sirius/ontology/hero/hasEstimate> ;
                                                      rdfs:comment "A property that assigns a estimate value for the best case scenario to a measure."@en ;
                                                      rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                      rdfs:label "has low estimate" .


###  http://purl.org/sirius/ontology/hero/hasNote
<http://purl.org/sirius/ontology/hero/hasNote> rdf:type owl:DatatypeProperty ;
                                               rdfs:range rdfs:Literal ;
                                               rdfs:comment "A property that assigns a textual description to something."@en ;
                                               rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                               rdfs:label "has note"@en .


###  http://purl.org/sirius/ontology/hero/hasProbableEstimate
<http://purl.org/sirius/ontology/hero/hasProbableEstimate> rdf:type owl:DatatypeProperty ;
                                                           rdfs:subPropertyOf <http://purl.org/sirius/ontology/hero/hasEstimate> ;
                                                           rdfs:comment "A property that assigns a estimate value for the most likely case scenario to a measure."@en ;
                                                           rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                           rdfs:label "has probable estimate" .


###  http://purl.org/sirius/ontology/hero/hasScore
<http://purl.org/sirius/ontology/hero/hasScore> rdf:type owl:DatatypeProperty ;
                                                rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/region.owl#hasRegionDataValue> ;
                                                rdfs:comment "A property that assigns a numerical estimate to quantify an asset value."@en ;
                                                rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                rdfs:label "has score"@en .


###  http://purl.org/sirius/ontology/hero/inDate
<http://purl.org/sirius/ontology/hero/inDate> rdf:type owl:DatatypeProperty ;
                                              rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#inDate> ;
                                              rdfs:comment "A property that assigns a datetime value to an observation."@en ;
                                              rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                              rdfs:label "in date"@en .


#################################################################
#    Classes
#################################################################

###  http://purl.org/sirius/ontology/hero/AgentOfDeterioration
<http://purl.org/sirius/ontology/hero/AgentOfDeterioration> rdf:type owl:Class ;
                                                            rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept> ;
                                                            rdfs:comment "A conceptualization used to classify a risk threatening an asset, made from the perspective of the asset itself."@en ;
                                                            rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                            rdfs:label "Agent of deterioration"@en .


###  http://purl.org/sirius/ontology/hero/AnalysisDescription
<http://purl.org/sirius/ontology/hero/AnalysisDescription> rdf:type owl:Class ;
                                                           rdfs:subClassOf <http://purl.org/sirius/ontology/hero/AssessmentActivity> ;
                                                           rdfs:comment "An activity that involves the quantification of risks to understand their magnitude and potential impact with respect to an asset."@en ;
                                                           rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                           rdfs:label "Analysis description"@en .


###  http://purl.org/sirius/ontology/hero/Aspect
<http://purl.org/sirius/ontology/hero/Aspect> rdf:type owl:Class ;
                                              rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept> ;
                                              rdfs:comment "A facet of the context that contributes to the overall understanding and assessment of the asset value."@en ;
                                              rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                              rdfs:label "Aspect"@en .


###  http://purl.org/sirius/ontology/hero/AssessmentActivity
<http://purl.org/sirius/ontology/hero/AssessmentActivity> rdf:type owl:Class ;
                                                          rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Event> ,
                                                                          <http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#TimeIndexedSituation> ;
                                                          rdfs:comment "A situation consisting in the assessment of the risks related to an asset."@en ;
                                                          rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                          rdfs:label "Assessment activity"@en .


###  http://purl.org/sirius/ontology/hero/ContextDescription
<http://purl.org/sirius/ontology/hero/ContextDescription> rdf:type owl:Class ;
                                                          rdfs:subClassOf <http://purl.org/sirius/ontology/hero/AssessmentActivity> ;
                                                          rdfs:comment "An activity that involves the examination of the contextual information of an asset."@en ;
                                                          rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                          rdfs:label "Context description"@en .


###  http://purl.org/sirius/ontology/hero/Criterion
<http://purl.org/sirius/ontology/hero/Criterion> rdf:type owl:Class ;
                                                 rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/region.owl#Region> ;
                                                 rdfs:comment "A conceptual category used as a basis for evaluating an asset during a risk assessment activity."@en ;
                                                 rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                 rdfs:label "Criterion"@en .


###  http://purl.org/sirius/ontology/hero/Dimension
<http://purl.org/sirius/ontology/hero/Dimension> rdf:type owl:Class ;
                                                 rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept> ;
                                                 rdfs:comment "A facet of an asset that contributes to the overall understanding and assessment of its value."@en ;
                                                 rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                 rdfs:label "Dimension"@en .


###  http://purl.org/sirius/ontology/hero/Exposure
<http://purl.org/sirius/ontology/hero/Exposure> rdf:type owl:Class ;
                                                rdfs:subClassOf <http://purl.org/sirius/ontology/hero/Measure> ;
                                                rdfs:comment "The size of the expected loss of value in the whole asset affected by the risk."@en ;
                                                rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                rdfs:label "Exposure"@en .


###  http://purl.org/sirius/ontology/hero/FractionalValueLoss
<http://purl.org/sirius/ontology/hero/FractionalValueLoss> rdf:type owl:Class ;
                                                           rdfs:subClassOf <http://purl.org/sirius/ontology/hero/Measure> ;
                                                           rdfs:comment "The size of the expected loss of value in each part of the asset affected by the risk."@en ;
                                                           rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                           rdfs:label "Fractional value loss" .


###  http://purl.org/sirius/ontology/hero/Frequency
<http://purl.org/sirius/ontology/hero/Frequency> rdf:type owl:Class ;
                                                 rdfs:subClassOf <http://purl.org/sirius/ontology/hero/Measure> ;
                                                 rdfs:comment "The likelihood or probability of a specific risk event occurring within a given period. It quantifies how often a particular risk is expected to materialize and helps assess the regularity or recurrence of the risk."@en ;
                                                 rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                 rdfs:label "Frequency" .


###  http://purl.org/sirius/ontology/hero/IdentificationDescription
<http://purl.org/sirius/ontology/hero/IdentificationDescription> rdf:type owl:Class ;
                                                                 rdfs:subClassOf <http://purl.org/sirius/ontology/hero/AssessmentActivity> ;
                                                                 rdfs:comment "An activity that involves the identification of risks to understand their nature, causes, and consequences with respect to an asset."@en ;
                                                                 rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                                 rdfs:label "Identification description"@en .


###  http://purl.org/sirius/ontology/hero/Layer
<http://purl.org/sirius/ontology/hero/Layer> rdf:type owl:Class ;
                                             rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept> ;
                                             rdfs:comment "A conceptualization used to classify a risk threatening an asset on the basis of the spatial context in which the asset exists."@en ;
                                             rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                             rdfs:label "Layer"@en .


###  http://purl.org/sirius/ontology/hero/Magnitude
<http://purl.org/sirius/ontology/hero/Magnitude> rdf:type owl:Class ;
                                                 rdfs:subClassOf <http://purl.org/sirius/ontology/hero/Measure> ;
                                                 rdfs:comment "The quantified extension of a risk, encompassing the overall impact and severity associated with it if it were to occur."@en ;
                                                 rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                 rdfs:label "Magnitude"@en .


###  http://purl.org/sirius/ontology/hero/Measure
<http://purl.org/sirius/ontology/hero/Measure> rdf:type owl:Class ;
                                               rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/region.owl#Region> ;
                                               rdfs:comment "A quantifiable aspect of something that expresses a more precise and standardised evaluation of it, based on assigning numerical values or metrics to some specific attributes."@en ;
                                               rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                               rdfs:label "Measure"@en .


###  http://purl.org/sirius/ontology/hero/Observation
<http://purl.org/sirius/ontology/hero/Observation> rdf:type owl:Class ;
                                                   rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Observation> ;
                                                   rdfs:comment "An observation about an asset during an assessment activity."@en ;
                                                   rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                   rdfs:label "Observation"@en .


###  http://purl.org/sirius/ontology/hero/Parameter
<http://purl.org/sirius/ontology/hero/Parameter> rdf:type owl:Class ;
                                                 rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Parameter> ;
                                                 rdfs:comment "A contextual parameter used in an observation about an asset during an assessment activity."@en ;
                                                 rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                                 rdfs:label "Parameter"@en .


###  http://purl.org/sirius/ontology/hero/Risk
<http://purl.org/sirius/ontology/hero/Risk> rdf:type owl:Class ;
                                            rdfs:subClassOf <http://purl.org/sirius/ontology/hero/Criterion> ;
                                            rdfs:comment "The possibility of a loss of value to an asset."@en ;
                                            rdfs:isDefinedBy "http://purl.org/sirius/ontology/hero" ;
                                            rdfs:label "Risk"@en .


###  http://purl.org/sirius/ontology/hero/RiskType
<http://purl.org/sirius/ontology/hero/RiskType> rdf:type owl:Class ;
