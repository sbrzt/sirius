<?xml version="1.0"?>
<rdf:RDF xmlns="http://purl.org/sirius/ontology/hero#"
     xml:base="http://purl.org/sirius/ontology/hero"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:cpannotationschema="http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#">
    <owl:Ontology rdf:about="http://purl.org/sirius/ontology/hero">
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/classification.owl"/>
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl"/>
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl"/>
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/region.owl"/>
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl"/>
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl"/>
        <owl:imports rdf:resource="http://xmlns.com/foaf/0.1/"/>
        <dc:creator>Barzaghi, Sebastian [0000-0002-0799-1527]</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2023-11-30</dc:date>
        <dc:description xml:lang="en">The Heritage Risk Assessment Ontology (HeRO, https://purl.org/sirius/ontology/hero) is an OWL 2 DL ontology that aims to provide a framework for the formal description of risk assessment activities related to heritage.</dc:description>
        <dc:title xml:lang="en">Heritage Risk Assessment Ontology</dc:title>
        <terms:rights xml:lang="en">This work is distributed under a Creative Commons Attribution License (http://creativecommons.org/licenses/by/4.0/).</terms:rights>
        <owl:versionInfo>1.0.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/creator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/date -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/date"/>
    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://purl.org/dc/terms/rights -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/rights"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/sirius/ontology/hero/analyses -->

    <owl:ObjectProperty rdf:about="http://purl.org/sirius/ontology/hero/analyses">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/sirius/ontology/hero/assesses"/>
        <rdfs:comment xml:lang="en">A relationship between an analysis and a risk that is being analysed.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">analyses</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/assesses -->

    <owl:ObjectProperty rdf:about="http://purl.org/sirius/ontology/hero/assesses">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#forEntity"/>
        <rdfs:comment xml:lang="en">A relationship between an assessment activity and something assessed by it, such as a criterion.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">assesses</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/assigns -->

    <owl:ObjectProperty rdf:about="http://purl.org/sirius/ontology/hero/assigns">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/sirius/ontology/hero/assesses"/>
        <rdfs:comment xml:lang="en">A relationship between a value assessment and an asset value that is being assessed.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">assigns</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/describes -->

    <owl:ObjectProperty rdf:about="http://purl.org/sirius/ontology/hero/describes">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/sirius/ontology/hero/hasParticipant"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/situation.owl#isSettingFor"/>
        <rdfs:comment xml:lang="en">A relationship between an assessment activity and the thing described in that assessment, such as an asset.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">describes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/documents -->

    <owl:ObjectProperty rdf:about="http://purl.org/sirius/ontology/hero/documents">
        <owl:inverseOf rdf:resource="http://purl.org/sirius/ontology/hero/isDocumentedBy"/>
        <rdfs:comment xml:lang="en">A relationship between a document and something it documents.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">documents</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/hasObservation -->

    <owl:ObjectProperty rdf:about="http://purl.org/sirius/ontology/hero/hasObservation">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/sirius/ontology/hero/assesses"/>
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#hasObservation"/>
        <owl:inverseOf rdf:resource="http://purl.org/sirius/ontology/hero/isObservationOf"/>
        <rdfs:comment xml:lang="en">A relationship between an assessment activity and an observation occurring during said activity.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">has observation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/hasParameter -->

    <owl:ObjectProperty rdf:about="http://purl.org/sirius/ontology/hero/hasParameter">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#hasParameter"/>
        <rdfs:comment xml:lang="en">A relationship between an observation happening during a contextualisation activity and a contextual parameter that is being observed as part of the context.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">has parameter</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/hasParticipant -->

    <owl:ObjectProperty rdf:about="http://purl.org/sirius/ontology/hero/hasParticipant">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#hasParticipant"/>
        <owl:inverseOf rdf:resource="http://purl.org/sirius/ontology/hero/isParticipantIn"/>
        <rdfs:comment xml:lang="en">A relationship between an assessment activity and a participant that takes part in it.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">has participant</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/identifies -->

    <owl:ObjectProperty rdf:about="http://purl.org/sirius/ontology/hero/identifies">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/sirius/ontology/hero/assesses"/>
        <rdfs:comment xml:lang="en">A relationship between a identification activity and a risk that is being identified.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">identifies</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/isClassifiedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/sirius/ontology/hero/isClassifiedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/classification.owl#isClassifiedBy"/>
        <rdfs:comment xml:lang="en">A relationship between a risk and another concept that may classify it, such as an agent of deterioration, a layer, or a type.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">is classified by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/isClassifiedByAgent -->

    <owl:ObjectProperty rdf:about="http://purl.org/sirius/ontology/hero/isClassifiedByAgent">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/sirius/ontology/hero/isClassifiedBy"/>
        <rdfs:comment xml:lang="en">A relationship between a risk and an agent of deterioration.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">is classified by agent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/isClassifiedByLayer -->

    <owl:ObjectProperty rdf:about="http://purl.org/sirius/ontology/hero/isClassifiedByLayer">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/sirius/ontology/hero/isClassifiedBy"/>
        <rdfs:comment xml:lang="en">A relationship between a risk and a layer.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">is classified by layer</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/isClassifiedByType -->

    <owl:ObjectProperty rdf:about="http://purl.org/sirius/ontology/hero/isClassifiedByType">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/sirius/ontology/hero/isClassifiedBy"/>
        <rdfs:comment xml:lang="en">A relationship between a risk and a type.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">is classified by type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/isDocumentedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/sirius/ontology/hero/isDocumentedBy">
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label>is documented by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/isObservationOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/sirius/ontology/hero/isObservationOf">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#isObservationOf"/>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">is observation of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/isParticipantIn -->

    <owl:ObjectProperty rdf:about="http://purl.org/sirius/ontology/hero/isParticipantIn">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#isParticipantIn"/>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">is participant in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/isQuantifiedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/sirius/ontology/hero/isQuantifiedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/region.owl#isRegionFor"/>
        <owl:inverseOf rdf:resource="http://purl.org/sirius/ontology/hero/quantifies"/>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">is quantified by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/quantifies -->

    <owl:ObjectProperty rdf:about="http://purl.org/sirius/ontology/hero/quantifies">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/region.owl#hasRegion"/>
        <rdfs:comment xml:lang="en">A relationship between an analysis activity and a measure considered during the analysis.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">quantifies</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/sirius/ontology/hero/assessesPercentage -->

    <owl:DatatypeProperty rdf:about="http://purl.org/sirius/ontology/hero/assessesPercentage">
        <rdfs:comment xml:lang="en">A property that assigns a value percentage to an asset which has been assigned a value in a value description.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">assessesPercentage</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/hasEstimate -->

    <owl:DatatypeProperty rdf:about="http://purl.org/sirius/ontology/hero/hasEstimate">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/region.owl#hasRegionDataValue"/>
        <rdfs:comment xml:lang="en">A property that assigns a estimate value to a measure.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">has estimate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/hasHighEstimate -->

    <owl:DatatypeProperty rdf:about="http://purl.org/sirius/ontology/hero/hasHighEstimate">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/sirius/ontology/hero/hasEstimate"/>
        <rdfs:comment xml:lang="en">A property that assigns a estimate value for the worst case scenario to a measure.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label>has high estimate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/hasLowEstimate -->

    <owl:DatatypeProperty rdf:about="http://purl.org/sirius/ontology/hero/hasLowEstimate">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/sirius/ontology/hero/hasEstimate"/>
        <rdfs:comment xml:lang="en">A property that assigns a estimate value for the best case scenario to a measure.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label>has low estimate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/hasNote -->

    <owl:DatatypeProperty rdf:about="http://purl.org/sirius/ontology/hero/hasNote">
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">A property that assigns a textual description to something.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">has note</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/hasProbableEstimate -->

    <owl:DatatypeProperty rdf:about="http://purl.org/sirius/ontology/hero/hasProbableEstimate">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/sirius/ontology/hero/hasEstimate"/>
        <rdfs:comment xml:lang="en">A property that assigns a estimate value for the most likely case scenario to a measure.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label>has probable estimate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/hasScore -->

    <owl:DatatypeProperty rdf:about="http://purl.org/sirius/ontology/hero/hasScore">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/region.owl#hasRegionDataValue"/>
        <rdfs:comment xml:lang="en">A property that assigns a numerical estimate to quantify an asset value.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">has score</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/sirius/ontology/hero/inDate -->

    <owl:DatatypeProperty rdf:about="http://purl.org/sirius/ontology/hero/inDate">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#inDate"/>
        <rdfs:comment xml:lang="en">A property that assigns a datetime value to an observation.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">in date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/sirius/ontology/hero/AgentOfDeterioration -->

    <owl:Class rdf:about="http://purl.org/sirius/ontology/hero/AgentOfDeterioration">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept"/>
        <rdfs:comment xml:lang="en">A conceptualization used to classify a risk threatening an asset, made from the perspective of the asset itself.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Agent of deterioration</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/sirius/ontology/hero/AnalysisDescription -->

    <owl:Class rdf:about="http://purl.org/sirius/ontology/hero/AnalysisDescription">
        <rdfs:subClassOf rdf:resource="http://purl.org/sirius/ontology/hero/AssessmentActivity"/>
        <rdfs:comment xml:lang="en">An activity that involves the quantification of risks to understand their magnitude and potential impact with respect to an asset.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Analysis description</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/sirius/ontology/hero/Aspect -->

    <owl:Class rdf:about="http://purl.org/sirius/ontology/hero/Aspect">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept"/>
        <rdfs:comment xml:lang="en">A facet of the context that contributes to the overall understanding and assessment of the asset value.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Aspect</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/sirius/ontology/hero/AssessmentActivity -->

    <owl:Class rdf:about="http://purl.org/sirius/ontology/hero/AssessmentActivity">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Event"/>
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#TimeIndexedSituation"/>
        <rdfs:comment xml:lang="en">A situation consisting in the assessment of the risks related to an asset.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Assessment activity</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/sirius/ontology/hero/ContextDescription -->

    <owl:Class rdf:about="http://purl.org/sirius/ontology/hero/ContextDescription">
        <rdfs:subClassOf rdf:resource="http://purl.org/sirius/ontology/hero/AssessmentActivity"/>
        <rdfs:comment xml:lang="en">An activity that involves the examination of the contextual information of an asset.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Context description</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/sirius/ontology/hero/Criterion -->

    <owl:Class rdf:about="http://purl.org/sirius/ontology/hero/Criterion">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/region.owl#Region"/>
        <rdfs:comment xml:lang="en">A conceptual category used as a basis for evaluating an asset during a risk assessment activity.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Criterion</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/sirius/ontology/hero/Dimension -->

    <owl:Class rdf:about="http://purl.org/sirius/ontology/hero/Dimension">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept"/>
        <rdfs:comment xml:lang="en">A facet of an asset that contributes to the overall understanding and assessment of its value.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Dimension</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/sirius/ontology/hero/Exposure -->

    <owl:Class rdf:about="http://purl.org/sirius/ontology/hero/Exposure">
        <rdfs:subClassOf rdf:resource="http://purl.org/sirius/ontology/hero/Measure"/>
        <rdfs:comment xml:lang="en">The size of the expected loss of value in the whole asset affected by the risk.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Exposure</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/sirius/ontology/hero/FractionalValueLoss -->

    <owl:Class rdf:about="http://purl.org/sirius/ontology/hero/FractionalValueLoss">
        <rdfs:subClassOf rdf:resource="http://purl.org/sirius/ontology/hero/Measure"/>
        <rdfs:comment xml:lang="en">The size of the expected loss of value in each part of the asset affected by the risk.</rdfs:comment>
        <rdfs:isDefinedBy>http://purl.org/sirius/ontology/hero</rdfs:isDefinedBy>
        <rdfs:label>Fractional value loss</rdfs:label>
