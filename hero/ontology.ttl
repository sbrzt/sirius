#################################################################

###  http://purl.org/sirius/ontology/hero/analyses
<http://purl.org/sirius/ontology/hero/analyses> rdf:type owl:ObjectProperty ;
                                                rdfs:subPropertyOf <http://purl.org/sirius/ontology/hero/assesses> ;
                                                owl:inverseOf <http://purl.org/sirius/ontology/hero/isAnalysedIn> ;
                                                rdfs:domain <http://purl.org/sirius/ontology/hero/AnalysisDescription> ;
                                                rdfs:range <http://purl.org/sirius/ontology/hero/Risk> ;
                                                rdfs:comment "The relationship between an analysis and a risk that is being analysed."@en ;
                                                rdfs:isDefinedBy <http://purl.org/sirius/ontology/hero> ;
                                                rdfs:label "analyses"@en .


###  http://purl.org/sirius/ontology/hero/assesses
<http://purl.org/sirius/ontology/hero/assesses> rdf:type owl:ObjectProperty ;
                                                rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#forEntity> ;
                                                owl:inverseOf <http://purl.org/sirius/ontology/hero/isAssessedIn> ;
                                                rdfs:domain <http://purl.org/sirius/ontology/hero/AssessmentActivity> ;
                                                rdfs:range owl:Thing ;
                                                rdfs:comment "A relationship between an assessment activity and an entity assessed by it, such as a criterion."@en ;
                                                rdfs:isDefinedBy <http://purl.org/sirius/ontology/hero> ;
                                                rdfs:label "assesses"@en .


###  http://purl.org/sirius/ontology/hero/assigns
<http://purl.org/sirius/ontology/hero/assigns> rdf:type owl:ObjectProperty ;
                                               rdfs:subPropertyOf <http://purl.org/sirius/ontology/hero/assesses> ;
                                               owl:inverseOf <http://purl.org/sirius/ontology/hero/isAssignedIn> ;
                                               rdfs:domain <http://purl.org/sirius/ontology/hero/ValueDescription> ;
                                               rdfs:range <http://purl.org/sirius/ontology/hero/Value> ;
                                               rdfs:comment "The relationship between a value assessment and an asset value that is being assessed."@en ;
                                               rdfs:isDefinedBy <http://purl.org/sirius/ontology/hero> ;
                                               rdfs:label "assigns"@en .


###  http://purl.org/sirius/ontology/hero/describes
<http://purl.org/sirius/ontology/hero/describes> rdf:type owl:ObjectProperty ;
                                                 rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/participation.owl#hasParticipant> ,
                                                                    <http://www.ontologydesignpatterns.org/cp/owl/situation.owl#isSettingFor> ;
                                                 owl:inverseOf <http://purl.org/sirius/ontology/hero/isDescribedIn> ;
                                                 rdfs:domain <http://purl.org/sirius/ontology/hero/AssessmentActivity> ;
                                                 rdfs:range owl:Thing ;
                                                 rdfs:comment "The relationship between an assessment activity and the entity described in that assessment, such as an asset."@en ;
                                                 rdfs:isDefinedBy <http://purl.org/sirius/ontology/hero> ;
                                                 rdfs:label "describes"@en .


###  http://purl.org/sirius/ontology/hero/documents
<http://purl.org/sirius/ontology/hero/documents> rdf:type owl:ObjectProperty ;
                                                 owl:inverseOf <http://purl.org/sirius/ontology/hero/isDocumentedBy> ;
                                                 rdfs:domain <http://xmlns.com/foaf/0.1/Document> ;
                                                 rdfs:range owl:Thing ;
                                                 rdfs:comment "The relationship between a document and an entity documented by it."@en ;
                                                 rdfs:isDefinedBy <http://purl.org/sirius/ontology/hero> ;
                                                 rdfs:label "documents"@en .


###  http://purl.org/sirius/ontology/hero/hasObservation
<http://purl.org/sirius/ontology/hero/hasObservation> rdf:type owl:ObjectProperty ;
                                                      rdfs:subPropertyOf <http://purl.org/sirius/ontology/hero/assesses> ,
                                                                         <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#hasObservation> ;
                                                      owl:inverseOf <http://purl.org/sirius/ontology/hero/isObservationOf> ;
                                                      rdfs:domain <http://purl.org/sirius/ontology/hero/AssessmentActivity> ;
                                                      rdfs:range <http://purl.org/sirius/ontology/hero/Observation> ;
                                                      rdfs:comment "The relationship between an assessment activity and an observation occurring during said activity."@en ;
                                                      rdfs:isDefinedBy <http://purl.org/sirius/ontology/hero> ;
                                                      rdfs:label "has observation"@en .


###  http://purl.org/sirius/ontology/hero/hasParameter
<http://purl.org/sirius/ontology/hero/hasParameter> rdf:type owl:ObjectProperty ;
                                                    rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#hasParameter> ;
                                                    rdfs:domain <http://purl.org/sirius/ontology/hero/Observation> ;
                                                    rdfs:range <http://purl.org/sirius/ontology/hero/Parameter> ;
                                                    rdfs:comment "A relationship between an observation happening during a contextualisation activity and a contextual parameter that is being observed as part of the context."@en ;
                                                    rdfs:isDefinedBy <http://purl.org/sirius/ontology/hero> ;
                                                    rdfs:label "has parameter"@en .


###  http://purl.org/sirius/ontology/hero/hasParticipant
<http://purl.org/sirius/ontology/hero/hasParticipant> rdf:type owl:ObjectProperty ;
                                                      rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/participation.owl#hasParticipant> ;
                                                      owl:inverseOf <http://purl.org/sirius/ontology/hero/isParticipantIn> ;
                                                      rdfs:domain <http://purl.org/sirius/ontology/hero/AssessmentActivity> ;
                                                      rdfs:range <http://xmlns.com/foaf/0.1/Agent> ;
                                                      rdfs:comment "The relationship between an assessment activity and a participant that takes part in it."@en ;
                                                      rdfs:isDefinedBy <http://purl.org/sirius/ontology/hero> ;
                                                      rdfs:label "has participant"@en .


###  http://purl.org/sirius/ontology/hero/identifies
<http://purl.org/sirius/ontology/hero/identifies> rdf:type owl:ObjectProperty ;
                                                  rdfs:subPropertyOf <http://purl.org/sirius/ontology/hero/assesses> ;
                                                  owl:inverseOf <http://purl.org/sirius/ontology/hero/isIdentifiedIn> ;
                                                  rdfs:domain <http://purl.org/sirius/ontology/hero/IdentificationDescription> ;
                                                  rdfs:range <http://purl.org/sirius/ontology/hero/Risk> ;
                                                  rdfs:comment "The relationship between a identification activity and a risk that is being identified."@en ;
                                                  rdfs:isDefinedBy <http://purl.org/sirius/ontology/hero> ;
                                                  rdfs:label "identifies"@en .


###  http://purl.org/sirius/ontology/hero/isAnalysedIn
<http://purl.org/sirius/ontology/hero/isAnalysedIn> rdf:type owl:ObjectProperty ;
                                                    rdfs:subPropertyOf <http://purl.org/sirius/ontology/hero/isAssessedIn> ;
                                                    rdfs:domain <http://purl.org/sirius/ontology/hero/Risk> ;
                                                    rdfs:range <http://purl.org/sirius/ontology/hero/AnalysisDescription> ;
                                                    rdfs:comment "The relationship between a risk that is being analysed and an analysis activity."@en ;
                                                    rdfs:isDefinedBy <http://purl.org/sirius/ontology/hero> ;
                                                    rdfs:label "is analysed in"@en .


###  http://purl.org/sirius/ontology/hero/isAssessedIn
<http://purl.org/sirius/ontology/hero/isAssessedIn> rdf:type owl:ObjectProperty ;
                                                    rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#hasTimeIndexedSetting> ;
                                                    rdfs:domain owl:Thing ;
                                                    rdfs:range <http://purl.org/sirius/ontology/hero/AssessmentActivity> ;
                                                    rdfs:comment "The relationship between an entity, such as a criterion, and a assessment activity assessing it."@en ;
                                                    rdfs:isDefinedBy <http://purl.org/sirius/ontology/hero> ;
                                                    rdfs:label "is assessed in"@en .


###  http://purl.org/sirius/ontology/hero/isAssignedIn
<http://purl.org/sirius/ontology/hero/isAssignedIn> rdf:type owl:ObjectProperty ;
                                                    rdfs:subPropertyOf <http://purl.org/sirius/ontology/hero/isAssessedIn> ;
                                                    rdfs:domain <http://purl.org/sirius/ontology/hero/Value> ;
                                                    rdfs:range <http://purl.org/sirius/ontology/hero/ValueDescription> ;
                                                    rdfs:comment "The relationship between an asset value that is being assessed and a value assessment."@en ;
                                                    rdfs:isDefinedBy <http://purl.org/sirius/ontology/hero> ;
                                                    rdfs:label "is assigned in"@en .


