                                                  rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/region.owl#hasRegion> ;
                                                  rdfs:domain <http://purl.org/sirius/ontology/hero/AnalysisDescription> ;
                                                  rdfs:range <http://purl.org/sirius/ontology/hero/Measure> ;
                                                  rdfs:comment "The relationship between an analysis activity and a measure considered during the analysis."@en ;
                                                  rdfs:isDefinedBy <http://purl.org/sirius/ontology/hero> ;
                                                  rdfs:label "quantifies"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/classification.owl#classifies
