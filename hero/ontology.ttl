                                                           rdfs:domain <http://purl.org/sirius/ontology/hero/Risk> ;
                                                           rdfs:range <http://purl.org/sirius/ontology/hero/Layer> ;
                                                           rdfs:comment "A relationship between a risk and a layer."@en ;
                                                           rdfs:isDefinedBy <http://purl.org/sirius/ontology/hero> ;
                                                           rdfs:label "is classified by layer"@en .


###  http://purl.org/sirius/ontology/hero/isClassifiedByType
<http://purl.org/sirius/ontology/hero/isClassifiedByType> rdf:type owl:ObjectProperty ;
                                                          rdfs:subPropertyOf <http://purl.org/sirius/ontology/hero/isClassifiedBy> ;
                                                          rdfs:domain <http://purl.org/sirius/ontology/hero/Risk> ;
                                                          rdfs:range <http://purl.org/sirius/ontology/hero/RiskType> ;
                                                          rdfs:comment "A relationship between a risk and a type."@en ;
                                                          rdfs:isDefinedBy <http://purl.org/sirius/ontology/hero> ;
                                                          rdfs:label "is classified by type"@en .


###  http://purl.org/sirius/ontology/hero/isDescribedIn
<http://purl.org/sirius/ontology/hero/isDescribedIn> rdf:type owl:ObjectProperty ;
