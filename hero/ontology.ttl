@base <http://purl.org/sirius/ontology/hero#> .

<http://purl.org/sirius/ontology/hero> rdf:type owl:Ontology ;
                                        owl:imports <http://www.ontologydesignpatterns.org/cp/owl/classification.owl> ,
                                                    <http://www.ontologydesignpatterns.org/cp/owl/observation.owl> ,
                                                    <http://www.ontologydesignpatterns.org/cp/owl/participation.owl> ,
                                                    <http://www.ontologydesignpatterns.org/cp/owl/region.owl> ,
                                                    <http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl> ,
                                                    <http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl> ;
                                        <http://purl.org/dc/elements/1.1/creator> "Barzaghi, Sebastian [0000-0002-0799-1527]" ;
                                        <http://purl.org/dc/elements/1.1/date> "2023-11-30"^^xsd:date ;
                                        <http://purl.org/dc/elements/1.1/description> "The Heritage Risk Assessment Ontology (HeRO, https://purl.org/sirius/ontology/hero) is an OWL 2 DL ontology that aims to provide a framework for the formal description of risk assessment activities related to heritage."@en ;
                                        <http://purl.org/dc/elements/1.1/title> "Heritage Risk Assessment Ontology"@en ;
                                        <http://purl.org/dc/terms/rights> "This work is distributed under a Creative Commons Attribution License (http://creativecommons.org/licenses/by/4.0/)."@en ;
                                        owl:versionInfo "1.0.0" .

