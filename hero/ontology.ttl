                                                                                           [ rdf:type owl:Restriction ;
                                                                                             owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#hasParameter> ;
                                                                                             owl:someValuesFrom <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Parameter>
                                                                                           ] ,
                                                                                           [ rdf:type owl:Restriction ;
                                                                                             owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#isObservationOf> ;
                                                                                             owl:someValuesFrom owl:Thing
                                                                                           ] ,
                                                                                           [ rdf:type owl:Restriction ;
                                                                                             owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#inDate> ;
                                                                                             owl:cardinality "1"^^xsd:nonNegativeInteger
                                                                                           ] ;
                                                                           rdfs:comment "A specific situation where some thing is observed with respect ot a set of parameters." ;
                                                                           rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/observation.owl> ;
                                                                           rdfs:label "Observation"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Parameter
<http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Parameter> rdf:type owl:Class ;
