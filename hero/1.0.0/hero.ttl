#################################################################
#    Classes
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept
<http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept> rdf:type owl:Class ;
                                                                          rdfs:comment """A concept is a Social Object. 
The classifies relation relates concepts to entities at some time, but time can only be expressed in the alternative pattern: timeindexedclassification.""" ;
                                                                          rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/classification.owl> ;
                                                                          rdfs:label "Concept"@en ,
                                                                                     "Concetto"@it .


###  http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Observation
<http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Observation> rdf:type owl:Class ;
                                                                           rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/situation.owl#Situation> ,
                                                                                           [ rdf:type owl:Restriction ;
                                                                                             owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#hasParameter> ;
                                                                                             owl:someValuesFrom <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Parameter>
                                                                                           ] ,
                                                                                           [ rdf:type owl:Restriction ;
                                                                                             owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#isObservationOf> ;
                                                                                             owl:someValuesFrom owl:Thing
                                                                                           ] ,
                                                                                           [ rdf:type owl:Restriction ;
                                                                                             owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#inDate> ;
                                                                                             owl:cardinality "1"^^xsd:nonNegativeInteger
                                                                                           ] ;
                                                                           rdfs:comment "A specific situation where some thing is observed with respect ot a set of parameters." ;
                                                                           rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/observation.owl> ;
                                                                           rdfs:label "Observation"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Parameter
<http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Parameter> rdf:type owl:Class ;
                                                                         rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/region.owl#Region> ;
                                                                         owl:disjointWith <http://www.ontologydesignpatterns.org/cp/owl/situation.owl#Situation> ;
                                                                         rdfs:comment "The parameters of an observation describe the context and content of the observation. For example in a medical context an observation of a patient may contain a set of symptoms, that are the parameters of that observation." ;
                                                                         rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/observation.owl> ;
                                                                         rdfs:label "Parameter"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Event
<http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Event> rdf:type owl:Class ;
                                                                       rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/participation.owl#hasParticipant> ;
                                                                                         owl:someValuesFrom <http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Object>
                                                                                       ] ;
                                                                       owl:disjointWith <http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Object> ;
                                                                       rdfs:comment "Any physical, social, or mental process, event, or state." ;
                                                                       rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/participation.owl> ;
                                                                       rdfs:label "Event"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Object
<http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Object> rdf:type owl:Class ;
                                                                        rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                                          owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/participation.owl#isParticipantIn> ;
                                                                                          owl:someValuesFrom <http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Event>
                                                                                        ] ;
                                                                        rdfs:comment "Any physical, social, or mental object, or substance" ;
                                                                        rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/participation.owl> ;
                                                                        rdfs:label "Object"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/region.owl#Region
<http://www.ontologydesignpatterns.org/cp/owl/region.owl#Region> rdf:type owl:Class ;
                                                                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                                   owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/region.owl#isRegionFor> ;
                                                                                   owl:allValuesFrom owl:Thing
                                                                                 ] ;
                                                                 rdfs:comment """Any region in a dimensional space (a dimensional space is a maximal Region), which can be used as a value for a quality of an Entity . For example, TimeInterval, SpaceRegion, PhysicalAttribute, Amount, SocialAttribute are all subclasses of Region. 
Regions are not data values in the ordinary knowledge representation sense; in order to get patterns for modelling data, see the properties: representsDataValue and hasDataValue""" ;
                                                                 rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/region.owl> ;
                                                                 rdfs:label "Region"@en ,
                                                                            "Regione"@it .


###  http://www.ontologydesignpatterns.org/cp/owl/situation.owl#Situation
<http://www.ontologydesignpatterns.org/cp/owl/situation.owl#Situation> rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#TimeIndexedSituation
<http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#TimeIndexedSituation> rdf:type owl:Class ;
                                                                                             rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/situation.owl#Situation> ,
                                                                                                             [ rdf:type owl:Restriction ;
                                                                                                               owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#atTime> ;
                                                                                                               owl:minCardinality "1"^^xsd:nonNegativeInteger
                                                                                                             ] ,
                                                                                                             [ rdf:type owl:Restriction ;
                                                                                                               owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#forEntity> ;
                                                                                                               owl:minCardinality "1"^^xsd:nonNegativeInteger
                                                                                                             ] ;
                                                                                             rdfs:comment "A Situation that is explicitly indexed at some time for at least one entity." ;
                                                                                             rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl> ;
                                                                                             rdfs:label "Time indexed situation"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#TimeInterval
<http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#TimeInterval> rdf:type owl:Class ;
                                                                             rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/region.owl#Region> ,
                                                                                             [ rdf:type owl:Restriction ;
                                                                                               owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#hasIntervalEndDate> ;
                                                                                               owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                                                                             ] ,
                                                                                             [ rdf:type owl:Restriction ;
                                                                                               owl:onProperty <http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#hasIntervalStartDate> ;
                                                                                               owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                                                                             ] ;
                                                                             rdfs:comment """Any Region in a dimensional space that aims at representing time.
It should be used when you want to talk about time as an object having some characteristics.""" ;
                                                                             rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl> ;
                                                                             rdfs:label "Intervallo di tempo"@it ,
                                                                                        "Time interval"@en .


###  http://www.w3.org/2002/07/owl#Thing
owl:Thing rdf:type owl:Class .


###  http://xmlns.com/foaf/0.1/Agent
<http://xmlns.com/foaf/0.1/Agent> rdf:type owl:Class ;
                                  rdfs:comment "An agent (eg. person, group, software or physical artifact)."@en ;
                                  rdfs:label "Agent"@en .


###  http://xmlns.com/foaf/0.1/Document
<http://xmlns.com/foaf/0.1/Document> rdf:type owl:Class ;
                                     rdfs:comment "A document."@en ;
                                     rdfs:label "Document"@en .


###  https://w3id.org/sirius/ontology/hero/AgentOfDeterioration
:AgentOfDeterioration rdf:type owl:Class ;
                      rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept> ;
                      rdfs:comment "A conceptualization used to classify a risk threatening an asset, made from the perspective of the asset itself."@en ;
                      rdfs:isDefinedBy <https://w3id.org/sirius/ontology/hero> ;
                      rdfs:label "Agent of deterioration"@en .


###  https://w3id.org/sirius/ontology/hero/AnalysisDescription
:AnalysisDescription rdf:type owl:Class ;
                     rdfs:subClassOf :AssessmentActivity ;
                     rdfs:comment "An activity that involves the quantification of risks to understand their magnitude and potential impact with respect to an asset."@en ;
                     rdfs:isDefinedBy <https://w3id.org/sirius/ontology/hero> ;
                     rdfs:label "Analysis description"@en .


###  https://w3id.org/sirius/ontology/hero/Aspect
:Aspect rdf:type owl:Class ;
        rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept> ;
        rdfs:comment "A facet of the context that contributes to the overall understanding and assessment of the asset value."@en ;
        rdfs:isDefinedBy <https://w3id.org/sirius/ontology/hero> ;
        rdfs:label "Aspect"@en .


###  https://w3id.org/sirius/ontology/hero/AssessmentActivity
:AssessmentActivity rdf:type owl:Class ;
                    rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Event> ,
                                    <http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#TimeIndexedSituation> ;
                    rdfs:comment "A situation consisting in the assessment of the risks related to an asset."@en ;
                    rdfs:isDefinedBy <https://w3id.org/sirius/ontology/hero> ;
                    rdfs:label "Assessment activity"@en .


###  https://w3id.org/sirius/ontology/hero/ContextDescription
:ContextDescription rdf:type owl:Class ;
                    rdfs:subClassOf :AssessmentActivity ;
                    rdfs:comment "An activity that involves the examination of the contextual information of an asset."@en ;
                    rdfs:isDefinedBy <https://w3id.org/sirius/ontology/hero> ;
                    rdfs:label "Context description"@en .


###  https://w3id.org/sirius/ontology/hero/Dimension
:Dimension rdf:type owl:Class ;
           rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept> ;
           rdfs:comment "A facet of an asset that contributes to the overall understanding and assessment of its value."@en ;
           rdfs:isDefinedBy <https://w3id.org/sirius/ontology/hero> ;
           rdfs:label "Dimension"@en .


###  https://w3id.org/sirius/ontology/hero/Element
:Element rdf:type owl:Class ;
