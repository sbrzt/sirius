                                                                            rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/situation.owl#isSettingFor> ;
                                                                            owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#isParameterOf> ;
                                                                            rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Observation> ;
                                                                            rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Parameter> ;
                                                                            rdfs:comment "The relationship between an Observation and a Parameter."@en ;
                                                                            rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/observation.owl> ;
                                                                            rdfs:label "has parameter"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/observation.owl#isObservationOf
<http://www.ontologydesignpatterns.org/cp/owl/observation.owl#isObservationOf> rdf:type owl:ObjectProperty ;
                                                                               rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/situation.owl#isSettingFor> ;
                                                                               rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Observation> ;
