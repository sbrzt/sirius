                                                                                 rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Object> ;
                                                                                 rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Event> ;
                                                                                 rdfs:comment "The relationship between an Object and an Event it participates in."@en ;
                                                                                 rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/participation.owl> ;
                                                                                 rdfs:label "is participant in"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/region.owl#hasRegion
<http://www.ontologydesignpatterns.org/cp/owl/region.owl#hasRegion> rdf:type owl:ObjectProperty ;
                                                                    owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/region.owl#isRegionFor> ;
                                                                    rdfs:domain owl:Thing ;
                                                                    rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/region.owl#Region> ;
                                                                    rdfs:comment "A relation between entities and regions, e.g. 'the number of wheels of that truck is 12'." ;
                                                                    rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/region.owl> ;
                                                                    rdfs:label "ha regione"@it ,
                                                                               "has region"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/region.owl#isRegionFor
<http://www.ontologydesignpatterns.org/cp/owl/region.owl#isRegionFor> rdf:type owl:ObjectProperty ;
                                                                      rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/region.owl#Region> ;
                                                                      rdfs:range owl:Thing ;
                                                                      rdfs:comment "A relation between entities and regions, e.g. 'the color of my car is red'." ;
                                                                      rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/region.owl> ;
                                                                      rdfs:label "is region for"@en ,
                                                                                 "è un ragione di"@it .


###  http://www.ontologydesignpatterns.org/cp/owl/situation.owl#hasSetting
<http://www.ontologydesignpatterns.org/cp/owl/situation.owl#hasSetting> rdf:type owl:ObjectProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/situation.owl#isSettingFor
<http://www.ontologydesignpatterns.org/cp/owl/situation.owl#isSettingFor> rdf:type owl:ObjectProperty ;
                                                                          rdfs:comment "The relationship between a Situation and something it is the setting for."@en .


###  http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#atTime
<http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#atTime> rdf:type owl:ObjectProperty ;
                                                                               rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/situation.owl#isSettingFor> ;
                                                                               owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#isTimeIndexFor> ;
                                                                               rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#TimeIndexedSituation> ;
                                                                               rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#TimeInterval> ;
                                                                               rdfs:comment "The relationship between a Time indexed situation and a Time interval."@en ;
                                                                               rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl> ;
                                                                               rdfs:label "at time"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#forEntity
<http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#forEntity> rdf:type owl:ObjectProperty ;
                                                                                  rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/situation.owl#isSettingFor> ;
                                                                                  owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#hasTimeIndexedSetting> ;
                                                                                  rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#TimeIndexedSituation> ;
                                                                                  rdfs:range owl:Thing ;
                                                                                  rdfs:comment "The relationship between a Time indexed situation and something contextualised in it."@en ;
                                                                                  rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl> ;
                                                                                  rdfs:label "for entity"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#hasTimeIndexedSetting
<http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#hasTimeIndexedSetting> rdf:type owl:ObjectProperty ;
                                                                                              rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/situation.owl#hasSetting> ;
                                                                                              rdfs:domain owl:Thing ;
                                                                                              rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#TimeIndexedSituation> ;
                                                                                              rdfs:comment "The relationship between an entity and a Time indexed situation contextualising it."@en ;
                                                                                              rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl> ;
                                                                                              rdfs:label "has time indexed setting"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#isTimeIndexFor
<http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#isTimeIndexFor> rdf:type owl:ObjectProperty ;
                                                                                       rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/situation.owl#hasSetting> ;
                                                                                       rdfs:domain <http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#TimeInterval> ;
                                                                                       rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#TimeIndexedSituation> ;
                                                                                       rdfs:comment "The relationship between a Time interval and a Time indexed situation."@en ;
                                                                                       rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl> ;
                                                                                       rdfs:label "is time index for"@en .


###  https://w3id.org/sirius/ontology/hero/analyses
:analyses rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :assesses ;
          owl:inverseOf :isAnalysedIn ;
          rdfs:domain :AnalysisDescription ;
          rdfs:range :Risk ;
          rdfs:comment "The relationship between an analysis and a risk that is being analysed."@en ;
          rdfs:isDefinedBy <https://w3id.org/sirius/ontology/hero> ;
          rdfs:label "analyses"@en .


###  https://w3id.org/sirius/ontology/hero/assesses
:assesses rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#forEntity> ;
          owl:inverseOf :isAssessedIn ;
          rdfs:domain :AssessmentActivity ;
          rdfs:range :Element ;
          rdfs:comment "A relationship between an assessment activity and an entity assessed by it, such as a criterion."@en ;
          rdfs:isDefinedBy <https://w3id.org/sirius/ontology/hero> ;
          rdfs:label "assesses"@en .


###  https://w3id.org/sirius/ontology/hero/assigns
:assigns rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :assesses ;
         owl:inverseOf :isAssignedIn ;
         rdfs:domain :ValueDescription ;
         rdfs:range :Value ;
         rdfs:comment "The relationship between a value assessment and an asset value that is being assessed."@en ;
         rdfs:isDefinedBy <https://w3id.org/sirius/ontology/hero> ;
         rdfs:label "assigns"@en .


###  https://w3id.org/sirius/ontology/hero/describes
:describes rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/participation.owl#hasParticipant> ,
                              <http://www.ontologydesignpatterns.org/cp/owl/situation.owl#isSettingFor> ;
           owl:inverseOf :isDescribedIn ;
           rdfs:domain :AssessmentActivity ;
           rdfs:range owl:Thing ;
           rdfs:comment "The relationship between an assessment activity and the entity described in that assessment, such as an asset."@en ;
           rdfs:isDefinedBy <https://w3id.org/sirius/ontology/hero> ;
           rdfs:label "describes"@en .


###  https://w3id.org/sirius/ontology/hero/documents
:documents rdf:type owl:ObjectProperty ;
           owl:inverseOf :isDocumentedBy ;
           rdfs:domain <http://xmlns.com/foaf/0.1/Document> ;
           rdfs:range owl:Thing ;
           rdfs:comment "The relationship between a document and an entity documented by it."@en ;
           rdfs:isDefinedBy <https://w3id.org/sirius/ontology/hero> ;
           rdfs:label "documents"@en .


###  https://w3id.org/sirius/ontology/hero/hasObservation
:hasObservation rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#hasObservation> ;
                owl:inverseOf :isObservationOf ;
                rdfs:domain :AssessmentActivity ;
                rdfs:range :Observation ;
                rdfs:comment "The relationship between an assessment activity and an observation occurring during said activity."@en ;
                rdfs:isDefinedBy <https://w3id.org/sirius/ontology/hero> ;
                rdfs:label "has observation"@en .


###  https://w3id.org/sirius/ontology/hero/hasParameter
:hasParameter rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#hasParameter> ;
              rdfs:domain :Observation ;
              rdfs:range :Parameter ;
              rdfs:comment "A relationship between an observation happening during a contextualisation activity and a contextual parameter that is being observed as part of the context."@en ;
              rdfs:isDefinedBy <https://w3id.org/sirius/ontology/hero> ;
              rdfs:label "has parameter"@en .


###  https://w3id.org/sirius/ontology/hero/hasParticipant
:hasParticipant rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/participation.owl#hasParticipant> ;
                owl:inverseOf :isParticipantIn ;
                rdfs:domain :AssessmentActivity ;
                rdfs:range <http://xmlns.com/foaf/0.1/Agent> ;
                rdfs:comment "The relationship between an assessment activity and a participant that takes part in it."@en ;
                rdfs:isDefinedBy <https://w3id.org/sirius/ontology/hero> ;
                rdfs:label "has participant"@en .


###  https://w3id.org/sirius/ontology/hero/identifies
:identifies rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :assesses ;
            owl:inverseOf :isIdentifiedIn ;
            rdfs:domain :IdentificationDescription ;
            rdfs:range :Risk ;
            rdfs:comment "The relationship between a identification activity and a risk that is being identified."@en ;
            rdfs:isDefinedBy <https://w3id.org/sirius/ontology/hero> ;
            rdfs:label "identifies"@en .


###  https://w3id.org/sirius/ontology/hero/isAnalysedIn
:isAnalysedIn rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :isAssessedIn ;
              rdfs:domain :Risk ;
              rdfs:range :AnalysisDescription ;
