                                                                              rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/situation.owl#hasSetting> ;
                                                                              owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#isObservationOf> ;
                                                                              rdfs:domain owl:Thing ;
                                                                              rdfs:range <http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Observation> ;
                                                                              rdfs:comment "The relationship between an entity and an Observation."@en ;
                                                                              rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/observation.owl> ;
                                                                              rdfs:label "has observation"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/observation.owl#hasParameter
