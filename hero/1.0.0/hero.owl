        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#TimeIndexedSituation"/>
        <rdfs:comment xml:lang="en">The relationship between an entity and a Time indexed situation contextualising it.</rdfs:comment>
