        <rdfs:isDefinedBy rdf:resource="https://w3id.org/sirius/ontology/hero"/>
        <rdfs:label xml:lang="en">has note</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/sirius/ontology/hero/hasProbableEstimate -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/sirius/ontology/hero/hasProbableEstimate">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/sirius/ontology/hero/hasEstimate"/>
        <rdfs:domain rdf:resource="https://w3id.org/sirius/ontology/hero/Measure"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">A property that assigns a estimate value for the most likely case scenario to a measure.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/sirius/ontology/hero"/>
        <rdfs:label>has probable estimate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/sirius/ontology/hero/hasScore -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/sirius/ontology/hero/hasScore">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/region.owl#hasRegionDataValue"/>
        <rdfs:domain rdf:resource="https://w3id.org/sirius/ontology/hero/Value"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment xml:lang="en">A property that assigns a numerical estimate to quantify an asset value.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/sirius/ontology/hero"/>
        <rdfs:label xml:lang="en">has score</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/sirius/ontology/hero/inDate -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/sirius/ontology/hero/inDate">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#inDate"/>
        <rdfs:domain rdf:resource="https://w3id.org/sirius/ontology/hero/Observation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment xml:lang="en">A property that assigns a datetime value to an observation.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/sirius/ontology/hero"/>
        <rdfs:label xml:lang="en">in date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept -->


    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept">
        <rdfs:comment>A concept is a Social Object. 
The classifies relation relates concepts to entities at some time, but time can only be expressed in the alternative pattern: timeindexedclassification.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/classification.owl"/>
        <rdfs:label xml:lang="en">Concept</rdfs:label>
        <rdfs:label xml:lang="it">Concetto</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Observation -->


    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Observation">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/situation.owl#Situation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#hasParameter"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Parameter"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#isObservationOf"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#inDate"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A specific situation where some thing is observed with respect ot a set of parameters.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl"/>
        <rdfs:label xml:lang="en">Observation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Parameter -->


    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Parameter">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/region.owl#Region"/>
        <owl:disjointWith rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/situation.owl#Situation"/>
        <rdfs:comment>The parameters of an observation describe the context and content of the observation. For example in a medical context an observation of a patient may contain a set of symptoms, that are the parameters of that observation.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl"/>
        <rdfs:label xml:lang="en">Parameter</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Event -->


    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Event">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#hasParticipant"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Object"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Object"/>
        <rdfs:comment>Any physical, social, or mental process, event, or state.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl"/>
        <rdfs:label xml:lang="en">Event</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Object -->


    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Object">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#isParticipantIn"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Event"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Any physical, social, or mental object, or substance</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl"/>
        <rdfs:label xml:lang="en">Object</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/region.owl#Region -->


    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/cp/owl/region.owl#Region">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/region.owl#isRegionFor"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Any region in a dimensional space (a dimensional space is a maximal Region), which can be used as a value for a quality of an Entity . For example, TimeInterval, SpaceRegion, PhysicalAttribute, Amount, SocialAttribute are all subclasses of Region. 
Regions are not data values in the ordinary knowledge representation sense; in order to get patterns for modelling data, see the properties: representsDataValue and hasDataValue</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/region.owl"/>
        <rdfs:label xml:lang="en">Region</rdfs:label>
        <rdfs:label xml:lang="it">Regione</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/situation.owl#Situation -->


    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/cp/owl/situation.owl#Situation"/>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#TimeIndexedSituation -->


    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#TimeIndexedSituation">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/situation.owl#Situation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#atTime"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#forEntity"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A Situation that is explicitly indexed at some time for at least one entity.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl"/>
        <rdfs:label xml:lang="en">Time indexed situation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#TimeInterval -->


    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#TimeInterval">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/region.owl#Region"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#hasIntervalEndDate"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#hasIntervalStartDate"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Any Region in a dimensional space that aims at representing time.
It should be used when you want to talk about time as an object having some characteristics.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl"/>
        <rdfs:label xml:lang="it">Intervallo di tempo</rdfs:label>
        <rdfs:label xml:lang="en">Time interval</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/2002/07/owl#Thing -->


    <owl:Class rdf:about="http://www.w3.org/2002/07/owl#Thing"/>
    


    <!-- http://xmlns.com/foaf/0.1/Agent -->


    <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Agent">
        <rdfs:comment xml:lang="en">An agent (eg. person, group, software or physical artifact).</rdfs:comment>
        <rdfs:label xml:lang="en">Agent</rdfs:label>
    </owl:Class>
    


    <!-- http://xmlns.com/foaf/0.1/Document -->


    <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Document">
        <rdfs:comment xml:lang="en">A document.</rdfs:comment>
        <rdfs:label xml:lang="en">Document</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sirius/ontology/hero/AgentOfDeterioration -->


    <owl:Class rdf:about="https://w3id.org/sirius/ontology/hero/AgentOfDeterioration">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept"/>
        <rdfs:comment xml:lang="en">A conceptualization used to classify a risk threatening an asset, made from the perspective of the asset itself.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/sirius/ontology/hero"/>
        <rdfs:label xml:lang="en">Agent of deterioration</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sirius/ontology/hero/AnalysisDescription -->


    <owl:Class rdf:about="https://w3id.org/sirius/ontology/hero/AnalysisDescription">
        <rdfs:subClassOf rdf:resource="https://w3id.org/sirius/ontology/hero/AssessmentActivity"/>
        <rdfs:comment xml:lang="en">An activity that involves the quantification of risks to understand their magnitude and potential impact with respect to an asset.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/sirius/ontology/hero"/>
        <rdfs:label xml:lang="en">Analysis description</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sirius/ontology/hero/Aspect -->


    <owl:Class rdf:about="https://w3id.org/sirius/ontology/hero/Aspect">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept"/>
        <rdfs:comment xml:lang="en">A facet of the context that contributes to the overall understanding and assessment of the asset value.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/sirius/ontology/hero"/>
        <rdfs:label xml:lang="en">Aspect</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sirius/ontology/hero/AssessmentActivity -->


    <owl:Class rdf:about="https://w3id.org/sirius/ontology/hero/AssessmentActivity">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Event"/>
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#TimeIndexedSituation"/>
        <rdfs:comment xml:lang="en">A situation consisting in the assessment of the risks related to an asset.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/sirius/ontology/hero"/>
        <rdfs:label xml:lang="en">Assessment activity</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sirius/ontology/hero/ContextDescription -->


    <owl:Class rdf:about="https://w3id.org/sirius/ontology/hero/ContextDescription">
        <rdfs:subClassOf rdf:resource="https://w3id.org/sirius/ontology/hero/AssessmentActivity"/>
        <rdfs:comment xml:lang="en">An activity that involves the examination of the contextual information of an asset.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/sirius/ontology/hero"/>
        <rdfs:label xml:lang="en">Context description</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sirius/ontology/hero/Dimension -->


    <owl:Class rdf:about="https://w3id.org/sirius/ontology/hero/Dimension">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept"/>
        <rdfs:comment xml:lang="en">A facet of an asset that contributes to the overall understanding and assessment of its value.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/sirius/ontology/hero"/>
        <rdfs:label xml:lang="en">Dimension</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sirius/ontology/hero/Element -->


    <owl:Class rdf:about="https://w3id.org/sirius/ontology/hero/Element">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/region.owl#Region"/>
        <rdfs:comment xml:lang="en">A conceptual category used as a basis for evaluating an asset during a risk assessment activity.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/sirius/ontology/hero"/>
        <rdfs:label xml:lang="en">Element</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sirius/ontology/hero/Exposure -->


    <owl:Class rdf:about="https://w3id.org/sirius/ontology/hero/Exposure">
        <rdfs:subClassOf rdf:resource="https://w3id.org/sirius/ontology/hero/Measure"/>
        <rdfs:comment xml:lang="en">The size of the expected loss of value in the whole asset affected by the risk.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/sirius/ontology/hero"/>
        <rdfs:label xml:lang="en">Exposure</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sirius/ontology/hero/FractionalValueLoss -->


    <owl:Class rdf:about="https://w3id.org/sirius/ontology/hero/FractionalValueLoss">
        <rdfs:subClassOf rdf:resource="https://w3id.org/sirius/ontology/hero/Measure"/>
        <rdfs:comment xml:lang="en">The size of the expected loss of value in each part of the asset affected by the risk.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/sirius/ontology/hero"/>
        <rdfs:label>Fractional value loss</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sirius/ontology/hero/Frequency -->


    <owl:Class rdf:about="https://w3id.org/sirius/ontology/hero/Frequency">
        <rdfs:subClassOf rdf:resource="https://w3id.org/sirius/ontology/hero/Measure"/>
        <rdfs:comment xml:lang="en">The likelihood or probability of a specific risk event occurring within a given period. It quantifies how often a particular risk is expected to materialize and helps assess the regularity or recurrence of the risk.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/sirius/ontology/hero"/>
        <rdfs:label>Frequency</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sirius/ontology/hero/IdentificationDescription -->


    <owl:Class rdf:about="https://w3id.org/sirius/ontology/hero/IdentificationDescription">
        <rdfs:subClassOf rdf:resource="https://w3id.org/sirius/ontology/hero/AssessmentActivity"/>
        <rdfs:comment xml:lang="en">An activity that involves the identification of risks to understand their nature, causes, and consequences with respect to an asset.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/sirius/ontology/hero"/>
        <rdfs:label xml:lang="en">Identification description</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sirius/ontology/hero/Layer -->


    <owl:Class rdf:about="https://w3id.org/sirius/ontology/hero/Layer">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept"/>
        <rdfs:comment xml:lang="en">A conceptualization used to classify a risk threatening an asset on the basis of the spatial context in which the asset exists.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/sirius/ontology/hero"/>
        <rdfs:label xml:lang="en">Layer</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sirius/ontology/hero/Magnitude -->


    <owl:Class rdf:about="https://w3id.org/sirius/ontology/hero/Magnitude">
        <rdfs:subClassOf rdf:resource="https://w3id.org/sirius/ontology/hero/Measure"/>
        <rdfs:comment xml:lang="en">The quantified extension of a risk, encompassing the overall impact and severity associated with it if it were to occur.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/sirius/ontology/hero"/>
        <rdfs:label xml:lang="en">Magnitude</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sirius/ontology/hero/Measure -->


    <owl:Class rdf:about="https://w3id.org/sirius/ontology/hero/Measure">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/cp/owl/region.owl#Region"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/sirius/ontology/hero/hasEstimate"/>
                        <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/region.owl#Region"/>
        <rdfs:comment xml:lang="en">A quantifiable aspect of some entity, that expresses a more precise and standardised evaluation of it, based on assigning numerical values or metrics to some specific attributes.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/sirius/ontology/hero"/>
        <rdfs:label xml:lang="en">Measure</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sirius/ontology/hero/Observation -->


    <owl:Class rdf:about="https://w3id.org/sirius/ontology/hero/Observation">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Observation"/>
        <rdfs:comment xml:lang="en">An observation about an asset during an assessment activity.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/sirius/ontology/hero"/>
        <rdfs:label xml:lang="en">Observation</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sirius/ontology/hero/Parameter -->


    <owl:Class rdf:about="https://w3id.org/sirius/ontology/hero/Parameter">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Parameter"/>
        <rdfs:comment xml:lang="en">A contextual parameter used in an observation about an asset during an assessment activity.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/sirius/ontology/hero"/>
        <rdfs:label xml:lang="en">Parameter</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sirius/ontology/hero/Risk -->


    <owl:Class rdf:about="https://w3id.org/sirius/ontology/hero/Risk">
        <rdfs:subClassOf rdf:resource="https://w3id.org/sirius/ontology/hero/Element"/>
        <rdfs:comment xml:lang="en">The possibility of a loss of value to an asset.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/sirius/ontology/hero"/>
        <rdfs:label xml:lang="en">Risk</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sirius/ontology/hero/RiskType -->


    <owl:Class rdf:about="https://w3id.org/sirius/ontology/hero/RiskType">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept"/>
        <rdfs:comment xml:lang="en">A conceptualization used to classify a risk according to its type of occurrence.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/sirius/ontology/hero"/>
        <rdfs:label xml:lang="en">Risk type</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/sirius/ontology/hero/Value -->


    <owl:Class rdf:about="https://w3id.org/sirius/ontology/hero/Value">
        <rdfs:subClassOf rdf:resource="https://w3id.org/sirius/ontology/hero/Element"/>
        <rdfs:comment xml:lang="en">A quality that represents the perceived significance or importance assigned to an asset.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/sirius/ontology/hero"/>
        <rdfs:label xml:lang="en">Value</rdfs:label>
    </owl:Class>
    


