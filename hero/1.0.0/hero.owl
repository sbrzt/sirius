    <!-- http://www.ontologydesignpatterns.org/cp/owl/classification.owl#classifies -->


    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/classification.owl#classifies">
        <owl:inverseOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/classification.owl#isClassifiedBy"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>A relation between a Concept and an Entity, e.g. the Role &apos;student&apos; classifies a Person &apos;John&apos;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/classification.owl"/>
        <rdfs:label xml:lang="it">classifica</rdfs:label>
        <rdfs:label xml:lang="en">classifies</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/classification.owl#isClassifiedBy -->


    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/classification.owl#isClassifiedBy">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/classification.owl#Concept"/>
        <rdfs:comment>A relation between a Concept and an Entity, e.g. &apos;John is considered a typical rude man&apos;; your last concert constitutes the achievement of a lifetime; &apos;20-year-old means she&apos;s mature enough&apos;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/classification.owl"/>
        <rdfs:label xml:lang="en">is classified by</rdfs:label>
        <rdfs:label xml:lang="it">è classificato da</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/observation.owl#hasObservation -->


    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#hasObservation">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/situation.owl#hasSetting"/>
        <owl:inverseOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#isObservationOf"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Observation"/>
        <rdfs:comment xml:lang="en">The relationship between an entity and an Observation.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl"/>
        <rdfs:label xml:lang="en">has observation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/observation.owl#hasParameter -->


    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#hasParameter">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/situation.owl#isSettingFor"/>
        <owl:inverseOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#isParameterOf"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Observation"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Parameter"/>
        <rdfs:comment xml:lang="en">The relationship between an Observation and a Parameter.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl"/>
        <rdfs:label xml:lang="en">has parameter</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/observation.owl#isObservationOf -->


    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#isObservationOf">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/situation.owl#isSettingFor"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Observation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="en">The relationship between an Observation and an entity it observes.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl"/>
        <rdfs:label xml:lang="en">is observation of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/observation.owl#isParameterOf -->


    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#isParameterOf">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/situation.owl#hasSetting"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Parameter"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl#Observation"/>
        <rdfs:comment xml:lang="en">The relationship between a Parameter and an Observation.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/observation.owl"/>
        <rdfs:label xml:lang="en">is parameter of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/participation.owl#hasParticipant -->


    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#hasParticipant">
        <owl:inverseOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#isParticipantIn"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Event"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Object"/>
        <rdfs:comment xml:lang="en">The relationship between a Event and a Object participating in it.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl"/>
        <rdfs:label xml:lang="en">has participant</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/participation.owl#isParticipantIn -->


    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#isParticipantIn">
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Object"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Event"/>
        <rdfs:comment xml:lang="en">The relationship between an Object and an Event it participates in.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl"/>
        <rdfs:label xml:lang="en">is participant in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/region.owl#hasRegion -->


    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/region.owl#hasRegion">
        <owl:inverseOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/region.owl#isRegionFor"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/region.owl#Region"/>
